						placeholder='Поиск новостей по ключевой фразе'
						autocomplete='true')

					+extendedSearch
						+esItem('Тематика')
							+select('subj')(
								multiple='multiple'
								id='subj')
								+sOption('mvd', 'МВД')
								+sOption('gibdd', 'ГИБДД')
								+sOption('medicine', 'Медицина')
								+sOption('finance', 'Финансы и рынки')
								+sOption('policy', 'Политика')
								+sOption('education', 'Образование')
								+sOption('military', 'Вооруженные силы')
								+sOption('realty', 'Недвижимость')
								+sOption('manRights', 'Права человека')
								+sOption('smallMidBusiness', 'Малый и средний бизнес')
					
						+esItem('Период публикации')
									+datepicker(
										id='publStart'
										mods=['half', 'start'])
									+datepicker(
										id='publEnd'
										mods=['half', 'end'])

						+esItem('Плюсы')(mods=['half'])
							+range('pluses')(mods=['green'])

						+esItem('Минусы')(mods=['half'])
